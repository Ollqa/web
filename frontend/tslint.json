{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "valid-jsdoc": true,
    "import-name": false,
    "react-hooks-nesting": "error"
  }
}
